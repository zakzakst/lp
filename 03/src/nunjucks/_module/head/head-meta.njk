<meta charset="utf-8">
<title>{{ PAGE_DATA.title }} | {{ SITE_DATA.name }}</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="description" content="{{ PAGE_DATA.description }}">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="canonical" href="{{ SITE_DATA.domain }}{{ PAGE_DATA.path }}">
<link rel="shortcut icon" href="{{ SITE_DATA.shortcutIcon }}" type="image/x-icon">
<link rel="apple-touch-icon" href="{{ SITE_DATA.appleIcon }}" type="image/png">

<!--OpenGraph-->
<meta property="og:locale" content="ja_JP">
<meta property="og:title" content="{{ PAGE_DATA.title }}">
<meta property="og:type" content="article">
<meta property="og:url" content="{{ SITE_DATA.domain }}{{ PAGE_DATA.path }}">
<meta property="og:site_name" content="{{ SITE_DATA.name }}">
<meta property="og:image" content="{{ SITE_DATA.ogImage }}">
<meta property="og:description" content="{{ PAGE_DATA.description }}">
